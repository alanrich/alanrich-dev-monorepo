{
  "npmScope": "alanrich",
  "affected": {
    "defaultBase": "main"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "installation": {
    "version": "latest",
    "plugins": []
  },
  "projects": {
    "alanrich-dev": {
      "root": "apps/alanrich-dev",
      "sourceRoot": "apps/alanrich-dev/src",
      "projectType": "application"
    },
    "anabasis": {
      "root": "apps/anabasis",
      "sourceRoot": "apps/anabasis/src",
      "projectType": "application"
    }
  },
  "targetDefaults": {
    "@nx/esbuild:esbuild": {
      "cache": true,
      "dependsOn": ["^build"],
      "inputs": ["default", "^default"]
    }
  },
  "plugins": [
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "lint"
      }
    },
    {
      "plugin": "@nx/jest/plugin",
      "options": {
        "targetName": "test"
      },
      "exclude": ["apps/anabasis-api-e2e/**/*"]
    }
  ]
}
