{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "npmScope": "alanrich",
  "affected": {
    "defaultBase": "main"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "test", "lint", "e2e"]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["default", "^default"],
      "cache": true
    }
  },
  "installation": {
    "version": "19.8.4",
    "plugins": []
  },
  "projects": {
    "alanrich-dev": {
      "root": "apps/alanrich-dev",
      "sourceRoot": "apps/alanrich-dev/src",
      "projectType": "application"
    },
    "anabasis": {
      "root": "apps/anabasis",
      "sourceRoot": "apps/anabasis/src",
      "projectType": "application"
    }
  }
}
