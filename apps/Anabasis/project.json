{
  "name": "anabasis",
  "root": "apps/anabasis",
  "sourceRoot": "apps/anabasis/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:node",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/anabasis",
        "main": "apps/anabasis/src/server/server.js",
        "outputFileName": "main.js",
        "tsConfig": "apps/anabasis/tsconfig.app.json",
        "assets": ["apps/anabasis/.env", "apps/anabasis/src/client"]
      }
    },
    "serve": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "anabasis:build"
      }
    }
  }
}
