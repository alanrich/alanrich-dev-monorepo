{
  "name": "alanrich-dev",
  "root": "apps/alanrich-dev",
  "sourceRoot": "apps/alanrich-dev/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/alanrich-dev",
        "index": "apps/alanrich-dev/public/index.html",
        "main": "apps/alanrich-dev/src/index.js",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/alanrich-dev/tsconfig.app.json",
        "assets": ["apps/alanrich-dev/src/assets", "apps/alanrich-dev/public"],
        "styles": [],
        "scripts": [],
        "webpackConfig": "apps/alanrich-dev/webpack.config.js"
      },
      "configurations": {
        "production": {
          "fileReplacements": [],
          "optimization": true,
          "sourceMap": false,
          "extractCss": true,
          "namedChunks": false,
          "aot": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "alanrich-dev:build"
      }
    }
  }
}
